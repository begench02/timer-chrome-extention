{
    "manifest_version": 3,
    "name": "Timer",
    "version": "0.1.0",
    "icons": {
      "16": "timer.png",
      "48": "timer.png",
      "128": "timer.png"
    },
    "permissions": ["storage", "alarms", "notifications"],
    "background": {
      "service_worker": "build/background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_title": "Timer",
      "default_icon": "timer.png"
    }
  }
  